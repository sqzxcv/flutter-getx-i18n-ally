// Place your settings in this file to overwrite default and user settings.
{
  "search.exclude": {
    "out": true,
    "dist": true,
    "node_modules": true,
    "CHANGELOG.md": true,
    "examples": true,
    "res": true,
    "screenshots": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cSpell.words": [
    "Exts",
    "Unflatten",
    "enablement",
    "exts",
    "inplace",
    "inplaces",
    "joomla",
    "lokalise",
    "regs",
    "youdao"
  ],
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": false,
  "i18n-ally.localesPaths": "./locales",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "flat",
  "i18n-ally.preferredDelimiter": "_",
  "i18n-ally.sortKeys": true,
  "i18n-ally.sortCompare": "binary",
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.translate.promptSource": true,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.translate.saveAsCandidates": true,
  "i18n-ally.keysInUse": [
    "view.progress_submenu.translated_keys",
    "view.progress_submenu.missing_keys",
    "view.progress_submenu.empty_keys"
  ],
  "i18n-ally.usage.scanningIgnore": [
    "examples",
  ],
  "i18n-ally.regex.usageMatchAppend": [
    "/\\*\\s*i18n\\s*\\*/\\s*['\"`]({key})['\"`]"
  ],
  "i18n-ally.enabledFrameworks": [
    "vscode",
    "vue",
    "general"
  ],
  "i18n-ally.theme.annotationMissingBorder": "#d37070",
  "i18n-ally.theme.annotationMissing": "#d37070",
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "flutter-getx-i18n-ally.localesPaths": [
    "locales",
    "examples/by-features/100-locales/locales",
    "examples/by-features/demo/locales",
    "examples/by-features/custom-locales-path/lang",
    "examples/by-features/collections/locales",
    "examples/by-features/dir-locales/locales",
    "examples/by-features/encoding/locales",
    "examples/by-features/extractions/locales",
    "examples/by-features/flatten-mode/lang",
    "examples/by-features/flatten-scope-conflict/lang",
    "examples/by-features/js-parser/locales",
    "examples/by-features/js-parser-dir/locales",
    "examples/by-features/json5/locales",
    "examples/by-features/linked/locales",
    "examples/by-features/tag-system-bcp47/locales",
    "examples/by-features/yaml/locales",
    "examples/by-frameworks/custom/locales",
    "examples/by-frameworks/ember/locales",
    "examples/by-frameworks/fluent-vue/i18n",
    "examples/by-frameworks/fluent-vue-sfc/i18n",
    "examples/by-frameworks/lingui-nextjs/locale",
    "examples/by-frameworks/next-intl/messages",
    "examples/by-frameworks/next-translate/locales",
    "examples/by-frameworks/ngneat-transloco/i18n",
    "examples/by-frameworks/non-i18n-project/locales",
    "examples/by-frameworks/php-po/locale",
    "examples/by-frameworks/svelte/messages",
    "examples/by-frameworks/vue-i18n/locales",
    "examples/by-frameworks/vue-i18n-sfc/locales",
    "examples/by-features/custom-scope-range/public/locales",
    "examples/by-features/path-matcher/locale-namespaces/i18n",
    "examples/by-features/path-matcher/namespaces-locale/i18n",
    "examples/by-frameworks/flutter/assets/i18n",
    "examples/by-frameworks/i18n-js/src/locales",
    "examples/by-frameworks/i18next-shopify/public/locales",
    "examples/by-frameworks/laravel/resources/lang",
    "examples/by-frameworks/next-international/src/locales",
    "examples/by-frameworks/ngx-translate/src/i18n",
    "examples/by-frameworks/react-i18next/public/locales",
    "examples/by-frameworks/ruby-on-rails/config/locales",
    "examples/by-frameworks/ui5/webapp/i18n",
    "examples/by-features/module-style/modules/moduleA/locales",
    "examples/by-features/module-style/modules/moduleB/locales",
    "examples/by-frameworks/vuex-i18n/todo-demo/app/i18n",
    "examples/by-features/module-style/modules/moduleC/nested/locales"
  ],
}
